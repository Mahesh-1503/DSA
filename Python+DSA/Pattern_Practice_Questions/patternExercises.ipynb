   "source": [
    "'''\n",
    "Example:\n",
    "\n",
    "Input: 3\n",
    "Output: ['***', '***', '***']\n",
    " \n",
    "Input: 5\n",
    "Output: ['*****', '*****', '*****', '*****', '*****']\n",
    "'''\n",
    "def generate_square(n):\n",
    "    return ['*'*n for _ in range(n)]\n",
    "n=int(input(\"Enter the side of the square: \"))\n",
    "print(generate_square(n))"
   ]
